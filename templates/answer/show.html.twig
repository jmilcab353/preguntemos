{% extends 'base.html.twig' %}

{% block title %}Answer{% endblock %}

{% block body %}
    <h1>Answer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ answer.id }}</td>
            </tr>
            <tr>
                <th>Timestamp</th>
                <td>{{ answer.timestamp ? answer.timestamp|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ChosenAnswer</th>
                <td>{{ answer.chosenAnswer }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_answer_index') }}">back to list</a>

    <a href="{{ path('app_answer_edit', {'id': answer.id}) }}">edit</a>

    {{ include('answer/_delete_form.html.twig') }}
{% endblock %}
